<script>
    $(document).ready(() => {
        init();
    })

    init = () => {
        getData();
    }

    getData = () => {
        $.ajax({
            headers: {
                'Accept': 'application/json',
                'Content-Type': 'application/json'
            },
            url: "/api/v1/getdata",
            type: "GET",
        }).done((data) => {
            console.log(data);
        }).fail((err) => {
            console.log(err);
        })
    }
</script>